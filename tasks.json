[
  {
    "label": "start lazygit",
    "command": "lazygit -p $ZED_WORKTREE_ROOT"
  },
  {
    "label": "cargo build",
    "command": "cargo build"
  },
  {
    "label": "cargo run",
    "command": "cargo run"
  },
  {
    "label": "cargo test",
    "command": "cargo test"
  },
  {
    "label": "file finder",
    "command": "zed \"$(tv files)\"",
    "hide": "always",
    "allow_concurrent_runs": true,
    "use_new_terminal": true,
    "shell": {
      "program": "sh"
    }
  }
]
